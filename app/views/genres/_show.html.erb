<ul id="tracks" class="<%= @genre.name.parameterize %>">
	 <% for track in @track_names %>
		<li><%= link_to "", "/tracks/play/#{track[:id]}", :class => "play-btn", :remote => true %><%= track[:name] %><%= link_to "", "/tracks/like/#{track[:id]}", :class => "like-btn", :remote => true %></li>
	<% end %>
</ul>
<div id="pagination"><%= will_paginate @tracks, :previous_label => '', :next_label => '' %></div>