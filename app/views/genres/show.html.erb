<div id="select-by">Popular: <a href="">This Week</a> | <a href="">This Month</a> | <a href="">All Time</a></div>
<ul id="tracks" class="<%= @genre.name.parameterize %>">
	<% Track.find_all_by_beatport_genre_id(@genre.beatport_genre_id, :limit => 20).each { |track|
			track_name = "#{track.artists.collect(&:name).join(', ')} - #{track.name} (#{track.mix_name})" 
		%>
		<li><a href="" class="play-btn">&nbsp;</a><%= track_name %><a href="" class="like-btn">&nbsp;</a></li>
	<% } %>
