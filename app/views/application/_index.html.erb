<ul id="tracks">
	 <% for track in @tracks %>
		<li class="<%= track.genre.name.parameterize %>"><%= link_to "", "/tracks/play/#{track.id}", :class => "play-btn", :remote => true %><%= track.full_name %><%= link_to "", "/tracks/like/#{track.id}", :class => "like-btn", :remote => true %></li>
	<% end %>
</ul>
<div id="pagination"><%= will_paginate @tracks, :previous_label => '', :next_label => '' %></div>